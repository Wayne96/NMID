<mapper namespace="cqupt.nmid.home.dao.impl.mybatis.SignUpMapper">
   <insert id="signUp">
   		insert into nmid_signup (name,email,qq,platform,introduction,phone,time) values(#{name},#{email},#{qq},#{platform},#{introduction},#{phone},#{time})
   		
   </insert>
</mapper>
